* {
  background-color: #212121;
  text-color: #eeeeee;
  font: "JetBrains Mono Nerd Font 11";
}

#window {
  padding: 8px;
}

#element {
  border: 0;
  padding: 2px 8px;
}

#element.selected.normal {
  background-color: #40c4ff;
  text-color: #212121;
}

#inputbar {
  padding: 0px 0px 10px 0px;
}

#entry {
  padding: 4px 8px;
}

#prompt {
  background-color: #69f0ae;
  padding: 4px 8px;
  text-color: #212121;
}
