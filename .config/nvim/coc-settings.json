    "Kanyon",
    "Kinded",
    "Konto",
    "Nord",
    "PPID",
    "Pangram",
    "Pavucontrol",
    "WSIs",
    "Yggdroot",
    "alacritty",
    "bimap",
    "brutto",
    "cocopon",
    "combinators",
    "contramap",
    "ctrlp",
    "ctrlpvim",
    "dunst",
    "effectful",
    "fehbg",
    "firestore",
    "fmap",
    "foldl",
    "goyo",
    "jiangmiao",
    "junegunn",
    "kolyamba",
    "linebreak",
    "linenr",
    "mempty",
    "monoid",
    "neoclide",
    "newtype",
    "nvim",
    "pactl",
    "pipeable",
    "pixelsize",
    "pulsemixer",
    "scrooloose",
    "scrot",
    "semigroup",
    "setxkbmap",
    "slock",
    "snoc",
    "struct",
    "tpope",
    "tupled",
    "unkiwii",
    "unwords",
    "windowset",
    "xbacklight",
    "xmobar",
    "xmonad",
    "xset",
    "xsetroot"
